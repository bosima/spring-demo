<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.bossma.springdemo.mybatismysql.mapper.ProductMapper">
    <select id="selectByName" parameterType="string" resultMap="cn.bossma.springdemo.mybatismysql.mapper.BaseProductMapper.BaseResultMap">
        select
        *
        from t_product
        where name=#{_parameter}
    </select>
</mapper>